 DEPFILE = src/MNT.p/main.cpp.o.d
 DEPFILE_UNQUOTED = src/MNT.p/main.cpp.o.d
 ARGS = -Isrc/MNT.p -Isrc -I../src -I../include -fdiagnostics-color=always -pipe -D_FILE_OFFSET_BITS=64 -Wall -Winvalid-pch -Wnon-virtual-dtor -std=c++17 -g

build src/MNT.p/Matrix.cpp.o: cpp_COMPILER ../src/Matrix.cpp
